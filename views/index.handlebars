
<!-- slider -->
<div class="slider-slick app-pages">
	<div class="slider-entry">

		<img src="img/slider1.png" alt="">
		<div class="overlay"></div>
		<div class="caption">
			<div class="container">
				<h2>Introducing Players Cup annual soccer league</h2>
				<p>Players Cup is an annual soccer league introduced by a bunch of likeminded folks, from the corporate background, to create a platform... </p>
				<a id="introReadBtn" class="button" href="intro">Read More</a>
			</div>
		</div>
	</div>
</div>
<!-- end slider -->

<!-- latest result -->
<div class="latest-result app-section app-section-top">
	<div class="container">
		<div class="app-title">
			<h4>Latest Result</h4>
			<div class="line"></div>
		</div>
		{{#each this.results}}
		<ul class="club-title">
			<li>{{team1}}</li>
			<li>{{team1Score}}</li>
			<li>:</li>
			<li>{{team2Score}}</li>
			<li>{{team2}}</li>
		</ul>
		<ul class="date">
			<li>{{displayDate}}</li>
		</ul>
		{{/each}}
	</div>
</div>
<!-- latest result -->

<!-- latest match -->
<div class="latest-match app-section app-bg-dark">
	<div class="container">
		<div class="app-title">
			<h4>Latest Match</h4>
			<div class="line"></div>
		</div>
		{{#each this.fixtures}}
		<ul class="club-title">
			<li>{{team1}}</li>
			<li>VS</li>
			<li>{{team2}}</li>
		</ul>
		<ul class="date">
			<li>{{displayDate}}</li>
		</ul>
		{{/each}}
	</div>
</div>
<!-- latest match -->

